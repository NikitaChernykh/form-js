{
  "$schema": "../../../form-json-schema/resources/schema.json",
  "id": "Form_1",
  "type": "default",
  "components": [
    {
      "id": "Text_1",
      "type": "text",
      "text": "# Address From"
    },
    {
      "id": "Address",
      "type": "group",
      "label": "Address",
      "path": "address",
      "showOutline": true,
      "components": [
        {
          "type": "textfield",
          "key": "AddressInfo1",
          "label": "Address 1"
        },
        {
          "type": "textfield",
          "key": "AddressInfo2",
          "label": "Address 2"
        },
        {
          "type": "textfield",
          "key": "CityInfo",
          "label": "City"
        },
        {
          "type": "textfield",
          "key": "StateInfo",
          "label": "State"
        },
        {
          "type": "zipcode",
          "key": "zipcode",
          "label": "Zipcode"
        },
        {
          "id": "Phonefield",
          "type": "phone",
          "key": "phone",
          "label": "Phone Number"
        }
      ]
    },
    {
      "id": "Button_1",
      "action": "submit",
      "label": "Submit",
      "type": "button"
    },
    {
      "id": "Button_2",
      "action": "reset",
      "label": "Reset",
      "type": "button"
    }
  ]
}